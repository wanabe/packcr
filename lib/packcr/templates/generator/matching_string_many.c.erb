if (
    pcc_refill_buffer(ctx, <%= n %>) < <%= n %> ||
<%- (n - 1).times do |i| -%>
    (ctx->buffer.buf + ctx->cur)[<%= i %>] != '<%= Packcr.escape_character(value[i]) %>' ||
<%- end -%>
    (ctx->buffer.buf + ctx->cur)[<%= n - 1 %>] != '<%= s = Packcr.escape_character(value[n - 1]) %>'
) goto L<%= "%04d" % onfail %>;
ctx->cur += <%= n %>;
