catch(<%= m %>, || {
    let p = self.input.position_offset;
    <%- if gen.location -%>
    TODO
    <%- end -%>
    <%- nodes.each_with_index do |expr, i| -%>
    <%-   c = i + 1 < nodes.length -%>
