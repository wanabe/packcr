    catch(<%= l %>, || {
        <%-   %><%= gen.generate_code(expr, l, 8, false, oncut: onfail) -%>
        <%-   if c -%>
        // unreachable codes omitted
        <%-   end -%>
        NOP
    })?
    <%-       break -%>
    <%-     else -%>
    catch(<%= l %>, || {
        <%-   %><%= gen.generate_code(expr, l, 8, false, oncut: onfail) -%>
        throw(<%= m %>)
    })?;
    <%-     end -%>
    <%-   end -%>
    self.input.position_offset = p;
    <%-   if gen.location -%>
    TODO
    <%-   end -%>
    <%-   if !c -%>
    throw(<%= onfail %>)
