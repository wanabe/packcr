1.times do |;pos, p_loc|
  pos = @cur
  <%- if gen.location -%>
  p_loc = @cur_loc
  <%- end -%>
  catch(<%= l %>) do
    <%- %><%= code -%>
    <%- if r != Packcr::CODE_REACH__ALWAYS_FAIL -%>
    @cur = pos
    <%-   if gen.location -%>
    @cur_loc = p_loc
    <%-   end -%>
    throw(<%= onfail %>)
    <%- end -%>
  end
  <%- if r != Packcr::CODE_REACH__ALWAYS_SUCCEED -%>
  @cur = pos
  <%-   if gen.location -%>
  @cur_loc = p_loc
  <%-   end -%>
  <%- end -%>
end
