<%- l = gen.next_label -%>
1.times do |;pos, p_loc, n|
  pos = @cur
  <%- if gen.location -%>
  p_loc = @cur_loc
  <%- end -%>
  n = chunk.thunks.length
  <%- r = expr.reachability -%>
  <%- if r != Packcr::CODE_REACH__ALWAYS_SUCCEED -%>
  <%-   m = gen.next_label -%>
  <%- end -%>
  catch(<%= m %>) do
    catch(<%= l %>) do
      <%- %><%= gen.generate_code_str(expr, l, 6, false) -%>
    <%- if r != Packcr::CODE_REACH__ALWAYS_SUCCEED -%>
      throw(<%= m %>)
    end
    <%-   if gen.location -%>
    @cur_loc = p_loc
    <%-   end -%>
    @cur = pos
    chunk.thunks[n..-1] = []
    <%- else -%>
    end
    <%- end -%>
  end
end
